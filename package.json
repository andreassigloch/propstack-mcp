{
  "name": "mcp-propstack",
  "version": "0.1.1",
  "description": "MCP server for PropStack real estate API integration",
  "author": "andreas@siglochconsulting",
  "license": "MIT",
  "type": "module",
  "mcpName": "io.github.sigloch/propstack",
  "main": "dist/index.js",
  "bin": {
    "mcp-propstack": "dist/index.js"
  },
  "files": [
